{
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.expand": false,
    "explorer.fileNesting.patterns": {
        "*.ts": "$(capture).js, $(capture).*.ts",
        "*.js": "$(capture).js.map, $(capture).min.js, $(capture).d.ts",
        "*.jsx": "$(capture).js",
        "*.tsx": "$(capture).ts",
        "*.feature": "$(capture).feature.*",
        "*.cshtml": "$(capture).cshtml.*",
        "*.razor": "$(capture).razor.*",
        "*.cs": "$(capture).*.cs",
        "azure-pipelines.yml": "azure-pipelines.*.yml",
        "docker-compose.yml": "docker-compose.*.yml",
        "docker-compose-*.yml": "docker-compose-$(capture).*.yml",
        "appsettings.json": "appsettings.*.json",
        "tsconfig.json": "tsconfig.*.json",
        "package.json": "package-lock.json, .npmrc, yarn.lock, .yarnrc, pnpm-lock.yaml"
    },
    "omnisharp.enableRoslynAnalyzers": true,
    "files.associations": {
        "*.csproj": "xml"
    },
    "workbench.editorAssociations": {
        "*.db": "sqlite-viewer.option"
    },
    "cSpell.enableFiletypes": [
        "aspnetcorerazor",
        "dbml",
        "dockercompose"
    ],
    "cSpell.words": [
        ".dbml",
        "addin",
        "ADDINS",
        "Antiforgery",
        "ASPNETCORE",
        "Behaviour",
        "Behaviours",
        "Blazor",
        "Bunit",
        "Cobertura",
        "cref",
        "datetimeoffset",
        "DBML",
        "dbml.",
        "DbmlNet",
        "DbNet",
        "endregion",
        "FromArgb",
        "Hsts",
        "Ical",
        "JSInvokable",
        "MediatR",
        "mpsc",
        "mspowerpoint",
        "nameof",
        "nologo",
        "Npgsql",
        "nvarchar",
        "Oidc",
        "onclick",
        "openapi",
        "paramref",
        "Refreshable",
        "stylesheet",
        "tbody",
        "text-nowrap",
        "thead",
        "Tynamix",
        "typeof",
        "typeparam",
        "uninstantiated",
        "UseHsts",
        "webp",
        "Xunit"
    ],
}